root: true
globals: {
  $: true,
  canvas: true,
  colorStringToHex: true,
  document: true,
  game: true,
  Hooks: true,
  loadTexture: true,
  PIXI: true,
  Token: true,
}
extends: "airbnb"
rules: {
  no-param-reassign: [ "error", { "props": false } ],
  no-shadow: [ "error", { "builtinGlobals": false, "hoist": "functions", "allow": [ "TokenFactions" ] } ],
  no-underscore-dangle: [ "error", { "allow": [ "_id", "_onChangeInput", "_geometry" ] } ],
}
